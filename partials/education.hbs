{{#if data.education.length}}
<section id="education">
  <div class="page__left__title title-color">Education</div>
  {{#each data.education}}
  <div>
    {{institution}},
    {{#if startDate}}
    <span class="startDate">
    {{startDate}} -
    </span>
    {{/if}}
    {{#if endDate}}
    <span class="endDate">
    {{endDate}}
    </span>
    {{else}}
    <span class="endDate">
    Present
    </span>
    {{/if}}
  </div>
  {{studyType}}
  {{/each}}
</section>
{{/if}}